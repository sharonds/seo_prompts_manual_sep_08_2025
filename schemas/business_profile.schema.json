{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Business Profile Schema â€” Output of Prompt 1",
  "type": "object",
  "properties": {
    "company_name": {
      "type": "string",
      "description": "Full company name"
    },
    "industry": {
      "type": "string",
      "description": "Primary industry"
    },
    "business_model": {
      "type": "string",
      "enum": [
        "B2B",
        "B2C",
        "B2B2C",
        "Marketplace"
      ],
      "description": "Business model type"
    },
    "core_products": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "Core products/services (simplified from detailed input)"
    },
    "value_props": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "Key value propositions"
    },
    "goals": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "Business goals and objectives"
    }
  },
  "required": [
    "company_name",
    "industry",
    "business_model",
    "core_products"
  ]
}